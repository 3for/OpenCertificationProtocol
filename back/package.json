{
  "name": "KYC",
  "version": "1.0.0",
  "description": "Bankex Server",
  "repository": "",
  "author": "ye5no",
  "license": "MIT",
  "scripts": {
    "lint": "eslint server",
    "lintFix": "eslint --fix server",
    "startDev": "cross-env NODE_ENV=development NODE_PATH=./server babel-node server/server.js",
    "development": "npm run lint && nodemon --exec npm run startDev",
    "test": "mocha --timeout 15000 tests/index-test.js",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./server",
    "build": "npm run clean && npm run build-server",
    "production": "node build/server.js",
    "staging": "node build/server.js",
    "cleanWin": "rmdir /q /s build && mkdir build",
    "buildWin": "npm run cleanWin && npm run build-server",
    "stagingWin": "cross-env NODE_ENV=staging NODE_PATH=./build node build/server.js",
    "productionWin": "cross-env NODE_ENV=production NODE_PATH=./build node build/server.js",
    "testDev": "cross-env NODE_ENV=development mocha --timeout 30000 tests/index-test.js",
    "testStage": "cross-env NODE_ENV=staging mocha --timeout 30000 tests/index-test.js",
    "testProd": "cross-env NODE_ENV=production mocha --timeout 30000 tests/index-test.js"
  },
  "nodemonConfig": {
    "watch": [
      "server/*"
    ],
    "delay": "1000"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "bcrypt": "^1.0.3",
    "bn.js": "^4.11.8",
    "chalk": "^2.4.1",
    "config": "^1.29.2",
    "cross-env": "^5.1.3",
    "emailjs": "^2.0.0",
    "js-yaml": "^3.10.0",
    "jsonwebtoken": "^8.1.1",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-ejs": "^4.1.0",
    "koa-favicon": "^2.0.0",
    "koa-logger": "^3.1.0",
    "koa-passport": "^4.0.1",
    "koa-router": "^7.3.0",
    "koa-session": "^5.8.2",
    "koa-socket": "^4.4.0",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.3",
    "lodash.pick": "^4.4.0",
    "mongoose": "^5.0.1",
    "mongoose-unique-validator": "^2.0.0",
    "nodemon": "^1.14.11",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "scrypt": "^6.0.3",
    "socket.io": "^2.0.4",
    "uuid": "^3.2.1",
    "web3": "1.0.0-beta.31",
    "ethereumjs-util": "^5.2.0",
    "is-buffer": "^2.0.3",
    "js-sha3": "^0.7.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.16.0",
    "eslint-config-google": "^0.9.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "socket.io-client": "^2.0.4"
  }
}
